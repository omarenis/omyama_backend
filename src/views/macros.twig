{% macro fieldset (significaton,label, input, classStyle, otherClasses) %}

    <fieldset class="row {{ classStyle=='form-inline' ? 'g-3': 'row' }} align-items-center">
        <legend>
            {{ signification }}
        </legend>
        <div class="col-auto">
            <label class=" col-form-label {{ classStyle == 'form-inline' }}">
                {{ label }}
            </label>
        </div>
        <div class="col-auto">
            {{ input }}
        </div>
    </fieldset>

{% endmacro %}
