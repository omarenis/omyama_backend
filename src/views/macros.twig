{% macro intervenors (intervenors) %}
    {% if intervenors == [] %}
    <p>
        no intervenors
    </p>
    {% else %}
    <div class="table table-responsive tableWrap">
        <table class="table align-items-center mb-0">
            <caption>

            </caption>
            <thead>
            <tr>
                <th scope="col"
                    class="text-center text-uppercase text-secondary text-xxs font-weight-bolder text-center">Id
                </th>
                <th scope="col"
                    class="text-uppercase text-secondary text-xxs font-weight-bolder text-center">titre de categorie
                </th>
                <th id="sku" scope="col"
                    class="text-uppercase text-secondary text-xxs font-weight-bolder ps-2 text-center">nombre de
                    produits Total
                </th>
                <th scope="col"
                    class="text-uppercase text-secondary text-xxs font-weight-bolder ps-2 text-center">nombre de
                    produits achetées
                </th>
                <th scope="col"
                    class="text-uppercase text-secondary text-xxs font-weight-bolder ps-2 text-center">number Purchases
                </th>
                <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-2 text-center"></th>
            </tr>
            </thead>
            <tbody>
            {% for intevenor in intervenors %}
            <tr>
                <td class="text-sm text-secondary mb-0 text-center">
                    {{ intervenor.id }}
                </td>
                <td class="text-sm text-secondary mb-0 text-center">
